<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="github" value="https://nuget.pkg.github.com/DarkgreyDevelopment/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <github>
            <add key="Username" value="{GITHUB_USERNAME}" />
            <add key="ClearTextPassword" value="{GITHUB_PAT}" />
            <!-- https://docs.github.com/en/packages/working-with-a-github-packages-registry/working-with-the-nuget-registry#authenticating-with-a-personal-access-token -->
        </github>
    </packageSourceCredentials>
</configuration>
